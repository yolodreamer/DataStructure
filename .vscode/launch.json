    "version": "0.2.0",
    "configurations": [
        {
            "name": "g++ - 生成和调试活动文件",
            "type": "cppdbg",
            "request": "launch",
            "program": "${fileDirname}/${fileBasenameNoExtension}.exe",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${fileDirname}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "为 gdb 启用整齐打印",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ],
            "preLaunchTask": "g++ build active file",
            "miDebuggerPath": "/usr/bin/gdb",
            "logging": {
                "trace": true,
                "traceResponse": true,
                "engineLogging": true,
                "programOutput": true,
                "exceptions": true
            },
            "launchCompleteCommand": "exec-run",
            "debugServerPath": "",
            "debugServerArgs": "",
            "serverStarted": "",
            "filterStderr": false,
            "filterStdout": false,
            "serverLaunchTimeout": 10000,
            "internalConsoleOptions": "openOnSessionStart",
            "showDisplayString": true,
            "visualizerFile": ""
        }
    ]
}